<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
<servlet>
	<servlet-name>ServletDemo</servlet-name>
	<servlet-class>com.likestar.servlet.ServletDemo</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>ServletDemo</servlet-name>
	<url-pattern>/demo</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>ConfigServletDemo</servlet-name>
	<servlet-class>com.likestar.servlet.ConfigServletDemo</servlet-class>
	<init-param>
		<param-name>name</param-name>
		<param-value>ShenJinXiang</param-value>
	</init-param>
	<init-param>
		<param-name>age</param-name>
		<param-value>18</param-value>
	</init-param>
	<init-param>
		<param-name>desc</param-name>
		<param-value>一个好人</param-value>
	</init-param>
</servlet>
<servlet-mapping>
	<servlet-name>ConfigServletDemo</servlet-name>
	<url-pattern>/configDemo</url-pattern>
</servlet-mapping>
</web-app>